{
  "name": "github-pr-dashboard",
  "version": "0.1.0",
  "description": "A pull request dashboard for GitHub.com.",
  "productName": "GitHub Pull Request Dashboard",
  "author": "derpierre65",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "vitest",
    "dev": "quasar dev",
    "build": "quasar build",
    "test:unit:ui": "vitest --ui",
    "test:unit:ci": "vitest run",
    "test:playwright:ci": "npx playwright test",
    "test:playwright:headed": "npx playwright test --headed",
    "test:playwright:ui": "npx playwright test --ui",
    "test:playwright:codegen": "npx playwright codegen",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "pinia": "^2.0.11",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@derpierre65/eslint-config": "github:derpierre65/eslint-config",
    "@playwright/test": "^1.56.1",
    "@quasar/app-vite": "^2.0.0-beta.12",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20.5.9",
    "@vitest/ui": "^3.1.2",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.2",
    "dayjs": "^1.11.18",
    "deepmerge": "^4.3.1",
    "jsep": "^1.4.0",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.5.3",
    "vite-plugin-generate-file": "^0.3.1",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
